	<xsd:simpleType name="Booleen">
		<xsd:annotation>
			<xsd:documentation>Déclaration d'un type normalisé conforme à CAM0066 pour les booléens</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:boolean">
			<xsd:pattern value="[0-1]"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="date">
		<xsd:annotation>
			<xsd:documentation>Déclaration d'un type normalisé conforme à CAM0074 pour les dates (forme SSAAMMJJ). ? indique que lorsque la date n'est pas renseignée, elle peut être à blanc</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="([0-9]{8})?"/>
		</xsd:restriction>
	</xsd:simpleType>
